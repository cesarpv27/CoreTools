<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- This project will output netstandard2.0 and net461 assemblies -->
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <PackageId>Miscellaneous.CoreTools</PackageId>
    <Version>1.1.0</Version>
    <Company>César Palomino Vila</Company>
    <Authors>César Palomino Vila</Authors>
    <Description>Contains and accumulates some base miscellaneous features that can help to develop other applications

TargetFrameworks: netstandard2.0; net461</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/cesarpv27/CoreTools</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryType></RepositoryType>
    <RepositoryUrl>https://github.com/cesarpv27/CoreTools</RepositoryUrl>
    <PackageReleaseNotes>EncodingExtensions class added with extension methods to enconde and decode into base64 enconding.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>
